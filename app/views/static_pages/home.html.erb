<div id="wrapper">
  <%= render "sidebar" %>
  <div id="page-content-wrapper">
    <%= render "/static_pages/flash_container" %>
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">
          <% if current_user != nil %>
            <h1>Hello <%= current_user.login_id %> ! Welcome</h1>
          <% else %>
            <h1>Hello Visitor ! Please log in</h1>
          <% end %>
          <%= render "/static_pages/menu_toggle" %>
        </div>
      </div>
    </div>
  </div>
</div>