<div id="wrapper">
  <%= render "/static_pages/sidebar" %>
  <div id="page-content-wrapper">
    <%= render "/static_pages/flash_container" %>
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-12">

			<h2>My Parsing Data Sequnce Files</h2>

			<h3>Pending pdsfs </h3>
			<table class="table table-bordered">
			  <thead>
			    <tr>
			      <th> Season Info </th>
			      <th> Null Ratio </th>
			      <th> Period Info </th>
			      <th> Estimate State </th>
			      <th> Total Tuple Num </th>
			      <th> Dup Tuple Num </th>
			      <th> Open File </th>
			      <th colspan="3"> Evaluate </th>
			    </tr>
			  </thead>
			  <tbody>
			    <% @my_pending_pdsfs.each do |pdsf| %>
			      <tr>
			        <td><%= pdsf.season_info %></td>
			        <td><%= pdsf.null_ratio %></td>
			        <td><%= pdsf.period_info %></td>
			        <td><%= pdsf.estimate_state %></td>
			        <td><%= pdsf.total_tuple_num %></td>
			        <td><%= pdsf.dup_tuple_num %></td>
			        <td> <!-- TODO --> </td>
			        <td> <!-- TODO : score --> </td>
			        <td> <!-- TODO : pass/non-pass --> </td>
			        <td> <!-- TODO : submit --> </td>
			      </tr>
			     <% end %>
			  </tbody>
			</table>

			<h3>Done pdsfs </h3>
			<table class="table table-bordered">
			  <thead>
			    <tr>
			      <th> Season Info </th>
			      <th> Null Ratio </th>
			      <th> Period Info </th>
			      <th> Estimate State </th>
			      <th> Total Tuple Num </th>
			      <th> Dup Tuple Num </th>
			      <th> Score </th>
			      <th> Passed </th>
			    </tr>
			  </thead>
			  <tbody>
			    <% @my_pending_pdsfs.each do |pdsf| %>
			      <tr>
			        <td><%= pdsf.season_info %></td>
			        <td><%= pdsf.null_ratio %></td>
			        <td><%= pdsf.period_info %></td>
			        <td><%= pdsf.estimate_state %></td>
			        <td><%= pdsf.total_tuple_num %></td>
			        <td><%= pdsf.dup_tuple_num %></td>
			        <td> <%= pdsf.score %> </td>
			        <% if pdsf.is_passed %>
			        <td> Y </td>
			        <% else %>
			        <td> N </td>
			        <% end %>
			      </tr>
			     <% end %>
			  </tbody>
			</table>   
			<br>
          <%= render "/static_pages/menu_toggle" %>
        </div>
      </div>
    </div>
  </div>
</div>
